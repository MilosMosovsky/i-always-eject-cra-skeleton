    "@types/jest": "^24.0.15",
    "@types/react": "^17.0.15",
    "jest": "^24.8.0",
    "npm-watch": "^0.6.0",
    "parcel": "^1.12.3",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.5.2"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router5": "^7.1.0",
    "router5": "^7.0.2",
    "router5-plugin-browser": "^7.0.2",
    "router5-plugin-logger": "^7.0.2",
    "styled-components": "^4.3.2"
  }
