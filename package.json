{
  "name": "kittyboom-clean",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel serve public/index.html --open",
    "ts:check": "tsc --allowJs --checkJs --noEmit -p tsconfig.json",
